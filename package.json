{
	"name": "@cfware/fastify-test-helper",
	"version": "0.1.0",
	"description": "Testing helper for @cfware/ava-selenium-manager with fastify",
	"main": "index.js",
	"scripts": {
		"test": "xo && nyc ava -sv"
	},
	"engines": {
		"node": ">=8"
	},
	"author": "Corey Farrell",
	"license": "MIT",
	"files": [
		"*.js"
	],
	"keywords": [],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cfware/fastify-test-helper.git"
	},
	"bugs": {
		"url": "https://github.com/cfware/fastify-test-helper/issues"
	},
	"homepage": "https://github.com/cfware/fastify-test-helper#readme",
	"dependencies": {
		"@babel/core": "^7.3.3",
		"babel-plugin-bare-import-rewrite": "^1.3.1",
		"babel-plugin-istanbul": "^5.1.1",
		"fastify": "^2.0.0",
		"fastify-babel": "^1.1.0",
		"fastify-static": "^2.3.4"
	},
	"devDependencies": {
		"@cfware/ava-selenium-manager": "^0.1.2",
		"@cfware/eslint-config-browser": "^0.1.2",
		"ava": "^1.2.1",
		"geckodriver": "^1.15.1",
		"lighterhtml": "^0.9.1",
		"nyc": "^13.3.0",
		"xo": "^0.24.0"
	},
	"xo": {
		"overrides": [
			{
				"files": [
					"test/fixtures/*.js"
				],
				"extends": "@cfware/eslint-config-browser"
			}
		]
	},
	"nyc": {
		"check-coverage": true,
		"per-file": true,
		"lines": 100,
		"statements": 100,
		"functions": 100,
		"branches": 100
	}
}
