{
	"name": "@cfware/fastify-test-helper",
	"version": "0.2.3",
	"description": "Testing helper for @cfware/ava-selenium-manager with fastify",
	"main": "index.js",
	"scripts": {
		"release": "standard-version --sign",
		"test": "xo && nyc ava -sv"
	},
	"engines": {
		"node": ">=8"
	},
	"author": "Corey Farrell",
	"license": "MIT",
	"files": [
		"*.js"
	],
	"keywords": [],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cfware/fastify-test-helper.git"
	},
	"bugs": {
		"url": "https://github.com/cfware/fastify-test-helper/issues"
	},
	"homepage": "https://github.com/cfware/fastify-test-helper#readme",
	"dependencies": {
		"@babel/core": "^7.4.3",
		"@babel/plugin-proposal-class-properties": "^7.4.0",
		"@babel/plugin-proposal-private-methods": "^7.4.3",
		"babel-plugin-bare-import-rewrite": "^1.5.0",
		"babel-plugin-istanbul": "^5.1.4",
		"babel-plugin-remove-ungap": "^1.0.0",
		"fastify": "^2.3.0",
		"fastify-babel": "^1.2.0",
		"fastify-static": "^2.4.0",
		"glob": "^7.1.3"
	},
	"devDependencies": {
		"@cfware/ava-selenium-manager": "^0.2.1",
		"@cfware/eslint-config-browser": "^0.3.0",
		"ava": "^1.4.1",
		"geckodriver": "^1.16.2",
		"lighterhtml": "^0.12.0",
		"nyc": "^14.1.0",
		"standard-version": "^6.0.1",
		"xo": "^0.24.0"
	},
	"xo": {
		"overrides": [
			{
				"files": [
					"test/fixtures/*.js"
				],
				"extends": "@cfware/eslint-config-browser"
			}
		]
	},
	"nyc": {
		"check-coverage": true,
		"per-file": true,
		"lines": 100,
		"statements": 100,
		"functions": 100,
		"branches": 100
	}
}
